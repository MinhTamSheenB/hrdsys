  object empQuery2: TADOQuery
    Connection = DMHrdsys.conn
    CursorType = ctStatic
    AfterScroll = empQueryAfterScroll
    Parameters = <>
    SQL.Strings = (
      'select t.emp_id,'
      '(select emp_name from vfhrdemp as v '
      'where v.emp_id=t.emp_id) as emp_name,'
      'dept_code,'
      'dept_title as dept_abbr,'
      'pst_code,'
      '(select pst_name from fhrd_prof as v '
      'where v.pst_code=t.pst_code) as pst_name,'
      't.sal_bas,t.sal_frn,t.bas_amt,t.frn_amt,t.local_amt,'
      't.valid_date '
      'from fhrd_tranhist as t,fhrd_salstru as s '
      'where t.emp_id=s.emp_id '
      'and s.tryout='#39'1'#39' order by t.emp_id,valid_date')
    Left = 40
    Top = 355
  end
