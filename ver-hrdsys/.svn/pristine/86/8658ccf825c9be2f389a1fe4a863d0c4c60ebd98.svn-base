  object empQuery: TADOQuery
    Connection = DMHrdsys.conn
    CursorType = ctStatic
    AfterScroll = empQueryAfterScroll
    Parameters = <>
    SQL.Strings = (
      
        'SELECT         e.emp_id, e.emp_name, e.dept_code, e.pst_code, e.' +
        'emp_etdt, '
      
        '                          e.emp_gvdt,s.sal_bas,s.sal_frn,s.local' +
        '_amt '
      'FROM             fhrd_emp e INNER JOIN'
      '                          fhrd_salstru s ON e.emp_id = s.emp_id'
      'WHERE         (GETDATE() >= DATEADD(month, 3, e.emp_gvdt)) '
      'AND (e.emp_info = '#39'0'#39') AND '
      '                          (s.tryout = '#39'0'#39')')
    Left = 136
    Top = 77
  end
