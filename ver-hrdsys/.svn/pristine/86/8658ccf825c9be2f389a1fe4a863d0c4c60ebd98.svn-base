  object fetchTransQry: TADOQuery
    Connection = DMHrdsys.conn
    CursorType = ctStatic
    Parameters = <>
    SQL.Strings = (
      'select vs.emp_id,'
      
        '(select emp_name from fhrd_emp where emp_id=vs.emp_id) as emp_na' +
        'me,'
      'vs.dept_code,'
      'vs.dept_abbr,'
      'vs.pst_code,'
      'vs.pst_name,'
      'vs.sal_bas,'
      '(select amount from fhrd_salgrd where '
      'vs.pst_code=fhrd_salgrd.pst_code '
      'and fhrd_salgrd.item_id='#39'01'#39' and '
      'fhrd_salgrd.grade=vs.sal_bas) '
      'as bas_amt,'
      'sal_frn,'
      '(select amount from fhrd_salgrd where '
      'vs.pst_code=fhrd_salgrd.pst_code '
      'and item_id='#39'02'#39' '
      'and fhrd_salgrd.grade=vs.sal_frn)'
      ' as frn_amt,'
      'vs.local_amt,'
      'vs.t_date '
      'from vfhrdsalgrd as vs'
      'where vs.tryout='#39'1'#39
      '')
    Left = 192
    Top = 325
  end
  object transDataSource: TDataSource
    DataSet = fetchTransQry
    Left = 232
    Top = 325
  end
