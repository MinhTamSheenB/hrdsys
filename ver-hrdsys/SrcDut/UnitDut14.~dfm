  object ADOQuery1: TADOQuery
    Connection = DMHrdsys.conn
    CursorType = ctStatic
    LockType = ltBatchOptimistic
    Parameters = <>
    Prepared = True
    SQL.Strings = (
      'select dept_code,dept_titl,act_num,'
      'act_num as normal_duty,act_num as overtime,'
      'act_num as stoptime,act_num as asktime,'
      'act_num as leaveTime,act_num as total,'
      'act_num as average,act_num as normalTime'
      ' from hrd_dept order by dept_code')
    Left = 168
