  object ADOQuery1: TADOQuery
    Connection = DMHrdsys.conn
    CursorType = ctStatic
    LockType = ltBatchOptimistic
    AfterScroll = ADOQuery1AfterScroll
    OnCalcFields = ADOQuery1CalcFields
    Parameters = <>
    SQL.Strings = (
      
        'select A.dept_code,min(B.abbr_titl) as abbr_titl,sum(tot) as peo' +
        'plesum,sum(A) as A_people,'
      '    sum(B) as B_people,sum(C) as C_people,sum(D) as D_people,'
      '    cast(sum(A)*100/sum(tot) as varchar)+'#39'%'#39' as A_ratio,'
      '    cast(sum(B)*100/sum(tot) as varchar)+'#39'%'#39' as B_ratio,'
      '    cast(sum(C)*100/sum(tot) as varchar)+'#39'%'#39' as C_ratio,'
      
        '    cast(sum(D)*100/sum(tot) as varchar)+'#39'%'#39' as D_ratio,'#39' '#39' as r' +
        'emark'
      '    from[tempdbf].[dbo].tmp_wrk_sum A,hrd_dept B '
      '   where A.dept_code=B.dept_code'
      '    group by A.dept_code')
    Left = 200
    Top = 2
